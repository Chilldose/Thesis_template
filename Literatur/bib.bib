@article{Reinhard2,
abstract = {The investigation of erosion rates of material compositions, used in nuclear fusion devices as a rst boundary between the plasma and the reactor vessel, is a topic of current research. Therefore tungsten and tungsten-nitride surfaces need to be bombarded with mono energetic deuterium particles to measure sputter yields at high uences, using a highly sensitive quartz crystal microbalance technique [1]. The existing ion source SOPHIE [2] cannot provide sucient high ion currents to do these measurements in an acceptable amount of time, so a new measurement setup needed to be developed. The rst setup uses a sputter gun as an ion source in combination with a Wien velocity lter to analyse the ion beam and to check if a setup without this lter is possible to reach even higher ion currents. The measurements with the velocity lter showed, that a maximum ux of 7  1016D=m2=s could be reached. This is about a factor 10 higher than the ux generated with SOPHIE. Furthermore it could be shown that a setup without the velocity lter should be possible, as long as the purity of the working gas source is high enough. Removing the velocity lter lead to a ux of 1  1018D=m2=s which makes high unece measurements possible.},
author = {{Reinhard Stadelmayer}},
file = {:C$\backslash$:/Users/Boss/Dropbox/Projektarbeit Aumayr/literatur/Unterlagen/Setup for High-Fluence Sputter Yield Measurements Projektarbeit.pdf:pdf},
